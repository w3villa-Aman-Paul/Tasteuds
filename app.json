{
  "expo": {
    "name": "Tastebuds - Lokalmat rett hjem",
    "slug": "Tastebuds-app-slug",
    "owner": "aman.paul",
    "version": "1.1.1",
    "orientation": "portrait",
    "icon": "./assets/images/appLogo.png",
    "scheme": "com.w3villa.tastebuds",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "splash": {
      "shown": false
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.w3villa.tastebuds",
      "buildNumber": "1.0.11",

      "usesAppleSignIn": true,
      "infoPlist": {
        "NSUserTrackingUsageDescription": true,
        "NSCameraUsageDescription": "This app uses the camera to take the profile image of user.",
        "SKAdNetworkItems": [
          {
            "SKAdNetworkIdentifier": "v9wttpbfk9.skadnetwork"
          },
          {
            "SKAdNetworkIdentifier": "n38lu8286q.skadnetwork"
          }
        ]
      }
    },
    "android": {
      "package": "com.w3villa.tastebuds",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.tastebuds",
          "enableGooglePay": true
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "3d3590cf-72f3-468a-bd76-b8d4dbd8934a"
      }
    }
  }
}
