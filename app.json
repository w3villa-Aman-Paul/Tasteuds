{
  "expo": {
    "name": "TasteBuds",
    "slug": "Tastebuds-app-slug",
    "owner": "aman.paul",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "mycoolredirect",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "splash": {
      "shown": false
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.w3villa.tastebuds",
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.575803309006-hqi5d55e9ge3vhund8kifreii46atgnc"
        }
      },

      "usesAppleSignIn": true
    },
    "android": {
      "package": "com.w3villa.tastebuds",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-facebook",
        {
          "userTrackingPermission": false
        }
      ]
    ]
  }
}
