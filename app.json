{
  "expo": {
    "name": "TasteBuds",
    "slug": "Tastebuds-app-slug",
    "owner": "aman.paul",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "splash": {
      "shown": false
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.w3villa.tastebuds",
      "usesAppleSignIn": true
    },
    "android": {
      "package": "com.w3villa.tastebuds",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["@react-native-google-signin/google-signin"]
  }
}
